{
  "name": "firebase_template",
  "version": "0.0.0",
  "authors": [
    "erickim928 <erickim928@gmail.com>"
  ],
  "description": "Boilerplate template for angularfire applications.",
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "AngularJS": "angular-latest#~1.4.5",
    "angular-chart.js": "~0.8.4",
    "angular-ui-router": "~0.2.15",
    "angular-ui-router-helper": "~0.0.10",
    "angular-ui-router.stateHelper": "~1.3.1",
    "angularfire": "~1.1.2",
    "firebase": "~2.2.9",
    "font-awesome": "fontawesome#~4.4.0",
    "jquery": "~2.1.4",
    "moment": "~2.10.6",
    "semantic": "semantic-ui#~2.1.3",
    "toastr": "~2.1.2"
  },
  "overrides": {
    "font-awesome": {
      "main": {
        "development": "css/font-awesome.css",
        "production": "css/font-awesome.min.css"
      }
    },
    "semantic": {
      "main": {
        "development": [
          "dist/semantic.css",
          "dist/semantic.js"
        ],
        "production": [
          "dist/semantic.min.css",
          "dist/semantic.min.js"
        ]
      }
    },
    "angular": {
      "main": {
        "development": "angular.js",
        "production": "angular.min.js"
      },
      "dependencies": {
        "jquery": "*"
      }
    },
    "angular-ui-router": {
      "main": {
        "development": "release/angular-ui-router.js",
        "production": "release/angular-ui-router.min.js"
      }
    },
    "angularfire": {
      "main": {
        "development": "dist/angularfire.js",
        "production": "dist/angularfire.min.js"
      }
    },
    "jquery": {
      "main": {
        "development": "dist/jquery.js",
        "production": "dist/jquery.min.js"
      }
    },
    "moment": {
      "main": {
        "development": "moment.js",
        "production": "min/moment.min.js"
      }
    },
    "toastr": {
      "main": {
        "development": [
          "toastr.css",
          "toastr.js"
        ],
        "production": [
          "toastr.min.css",
          "toastr.min.js"
        ]
      }
    },
	"angular-chart.js": {
      "main": {
        "development": [
          "dist/angular-chart.css",
          "dist/angular-chart.js"
        ],
        "production": [
          "dist/angular-chart.min.css",
          "dist/angular-chart.min.js"
        ]
      }
    }
  },
  "resolutions": {
    "angular": "1.3.x || 1.4.x"
  }
}
